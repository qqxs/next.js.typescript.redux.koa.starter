{
  "name": "react.next",
  "version": "0.0.1",
  "description": "next.js typescript redux demo",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./index",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "restart": "pm2 restart ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "fix": "eslint --fix src/**/*.ts --fix  src/**/*.tsx --fix src/**/*.js --fix  src/**/*.jsx",
    "lint": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,less}\""
  },
  "keywords": [],
  "author": "shineshao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@koa/router": "^8.0.8",
    "koa": "^2.11.0",
    "next": "^9.5.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass": "^1.27.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "cross-env": "^7.0.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "nodemon": "^2.0.2",
    "prettier": "2.1.2",
    "redux-devtools-extension": "^2.13.8"
  },
  "engines": {
    "node": " >= 11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx|js|jsx)": [
      "prettier --write"
    ],
    "*.+(json|css|scss|less|md)": [
      "prettier --write"
    ]
  }
}
